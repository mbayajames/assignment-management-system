{
    "app": {
      "name": "Assignment Management System",
      "version": "1.0.0",
      "env": "development",
      "port": 3000,
      "baseUrl": "http://localhost:3000"
    },
    "jwt": {
      "secret": "your_jwt_secret_key",
      "expiresIn": "1d"
    },
    "email": {
      "service": "gmail",
